  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "yeoman",
      "program": "${workspaceRoot}/node_modules/yo/lib/cli.js",
      "args": ["./generators/app/index.js"],
      "stopOnEntry": false,
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
