{
  "name": "@totalsoft/generator-graphql-rocket",
  "version": "0.0.0",
  "description": "GraphQL server sample with Apollo Server, Koa and token validation.",
  "homepage": "https://github.com/osstotalsoft/generator-graphql-rocket",
  "author": {
    "name": "Totalsoft",
    "url": "https://github.com/osstotalsoft"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "GraphQL",
    "Apollo-Server",
    "Koa",
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "^3.1.1",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-xo": "^0.39.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gh-release": "^6.0.4",
    "lint-staged": "^12.1.2"
  },
  "engines": {
    "npm": ">= 4.0.0",
    "node": ">= 14.15.0"
  },
  "dependencies": {
    "boxen": "5.1.2",
    "chalk": "4.1.2",
    "ejs-lint": "^1.2.2",
    "gulp-filter": "^7.0.0",
    "jest": "27.0.6",
    "jest-cli": "27.0.6",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "ramda": "^0.28.0",
    "rimraf": "^3.0.2",
    "through2": "^4.0.2",
    "update-notifier": "^5.1.0",
    "yeoman-assert": "^3.1.1",
    "yeoman-generator": "^5.7.0",
    "yeoman-test": "6.1.0",
    "yo": "^4.3.0",
    "yosay": "^2.0.2"
  },
  "scripts": {
    "test": "jest --runInBand --detectOpenHandles --collectCoverage",
    "test:coverage": "jest --collectCoverage",
    "eslint:report": "eslint --fix --ext .js .",
    "ejslint": "ejslint generators/app/templates/infrastructure",
    "release": "git push --follow-tags && gh-release -y"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/generators/app/templates",
      "<rootDir>/.tmp"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/generators/app/templates",
      "<rootDir>/.tmp"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/generators/app/templates",
      "<rootDir>/.tmp"
    ]
  },
  "repository": "https://github.com/osstotalsoft/generator-graphql-rocket.git",
  "license": "MIT"
}
