{
  "name": "@totalsoft/generator-graphql-rocket",
  "version": "0.0.0",
  "description": "GraphQL server sample with Apollo Server, Koa and token validation.",
  "homepage": "https://github.com/osstotalsoft/generator-graphql-rocket",
  "author": {
    "name": "Totalsoft",
    "url": "https://github.com/osstotalsoft"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "GraphQL",
    "Apollo-Server",
    "Koa",
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "^3.1.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-xo": "^0.35.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "gh-release": "^5.0.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^5.1.0"
  },
  "engines": {
    "npm": ">= 4.0.0",
    "node": ">= 14.15.0"
  },
  "dependencies": {
    "boxen": "^5.0.0",
    "chalk": "^4.1.0",
    "gulp-filter": "^6.0.0",
    "jest-cli": "^26.6.3",
    "prettier": "^2.2.1",
    "ramda": "^0.27.1",
    "rimraf": "^3.0.2",
    "through2": "^4.0.2",
    "update-notifier": "^5.1.0",
    "yeoman-generator": "4.13.0",
    "yo": "^3.1.1",
    "yosay": "^2.0.2"
  },
  "scripts": {
    "test": "jest --runInBand --detectOpenHandles --collectCoverage",
    "test:coverage": "jest --collectCoverage",
    "eslint:report": "eslint --fix --ext .js .",
    "release": "git push --follow-tags && gh-release -y"
  },
  "lint-staged": {
    "**/*": "prettier --write"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/generators/app/templates",
      "<rootDir>/.tmp"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/generators/app/templates",
      "<rootDir>/.tmp"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/generators/app/templates",
      "<rootDir>/.tmp"
    ]
  },
  "repository": "https://github.com/osstotalsoft/generator-graphql-rocket.git",
  "license": "MIT"
}
