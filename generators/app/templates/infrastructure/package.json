{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=14.15.0",
    "<%=packageManager%>": ">= <%= packageManagerVersion %>"
  },
  "scripts": {
    "start": "nodemon src/index.js",
    "test": "jest --collectCoverage --passWithNoTests",
    "test:watchAll": "<%=packageManager%> run test -- --watchAll",
    "test:watch": "<%=packageManager%> run test -- --watch",
    "test:ci": "cross-env CI=true <%=packageManager%> test -- --reporters=default --reporters=jest-junit --coverage --coverageReporters=cobertura --coverageReporters=lcov --coverageReporters=html",
    "lint": "eslint --fix --ext .js .",
    "prettier": "prettier --write **/*.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.+(js|md|css|graphql|json)": "prettier --write"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.3.3",
    "@graphql-tools/load": "^7.5.0",
    "@graphql-tools/schema": "^8.3.1",
    "@koa/router": "^10.1.1",
    <%_ if(dataLayer === "prisma") {_%>
    "@prisma/client": "^3.6.0",
    "prisma": "^3.6.0",
    <%_} else if(dataLayer === "knex") { _%>
      <%_ if(withMultiTenancy) {_%>
    "@totalsoft/knex-filters": "^2.0.0",
      <%_}_%>
    "dataloader": "^2.0.0",
    "datasource-sql": "^1.4.1",
    "knex": "^0.21.19",
    "knex-tiny-logger": "^2.1.0",
    "mssql": "^6.2.0",
    <%_}_%>
    <%_ if(addMessaging || (dataLayer === "knex" && withMultiTenancy && addSubscriptions)) {_%>
    "@totalsoft/message-bus": "^2.1.0",
    "@totalsoft/messaging-host": "^2.1.0",
    <%_}_%>
    "apollo-datasource": "^3.3.0",
    "apollo-datasource-rest": "^3.4.0",
    "apollo-server": "3.5.0",
    "apollo-server-koa": "3.5.0",
    "async-mutex": "^0.3.2",
    "bluebird": "3.7.2",
    "bufferutil": "^4.0.5",
    "colors": "^1.4.0",
    "console-stamp": "^3.0.3",
    "dotenv": "10.0.0",
    "get-stream": "^6.0.1",
    "graphql": "15.8.0",
    "graphql-middleware": "^6.1.13",
    <%_ if(addSubscriptions){ _%>
    "graphql-redis-subscriptions": "^2.4.0",
    "graphql-subscriptions": "^1.2.1",
    "ioredis": "^4.27.6",
    "subscriptions-transport-ws": "^0.9.19",
    <%_}_%>
    "graphql-shield": "^7.5.0",
    "graphql-tools": "^8.2.0",
    "graphql-upload": "^13.0.0",
    "humps": "^2.0.1",
    <%_ if(addTracing){ _%>
    "jaeger-client": "^3.18.1",
    "opentracing": "^0.14.5",
    "apollo-opentracing": "^2.1.0",
    <%_}_%>
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "^2.0.5",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-jwt": "^4.0.3",
    "lodash.merge": "^4.6.2",
    "mime-types": "^2.1.34",
    "node-nats-streaming": "0.3.2",
    "path": "^0.12.7",
    "pino": "^7.5.1",
    "ramda": "^0.27.1",
    "tedious": "^14.0.0",
    "utf-8-validate": "^5.0.7",
    "utf8-bytes": "0.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "easygraphql-tester": "^6.0.1",
    "eslint": "^8.4.1",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "jest-extended": "^1.2.0",
    "jest-junit": "^13.0.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended"
    ]
  }
}
