{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=14.15.0",
    "<%=packageManager%>": ">= <%= packageManagerVersion %>"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "jest --collectCoverage --passWithNoTests",
    "test:watchAll": "<%=packageManager%> run test -- --watchAll",
    "test:watch": "<%=packageManager%> run test -- --watch",
    "test:ci": "cross-env CI=true <%=packageManager%> test -- --reporters=default --reporters=jest-junit --coverage --coverageReporters=cobertura --coverageReporters=lcov --coverageReporters=html",
    "lint": "eslint --fix --ext .js .",
    "prettier": "prettier --write **/*.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.+(js|md|css|graphql|json)": "prettier --write"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^6.2.7",
    "@graphql-tools/load": "^6.2.8",
    "@koa/router": "^10.0.0",
    <%_ if(withMultiTenancy) {_%>
    "@totalsoft/knex-filters": "^1.0.1",
    <%_}_%>
    <%_ if((addSubscriptions && withMultiTenancy) || addMessaging) {_%>
    "@totalsoft/message-bus": "^1.0.0",
    <%_}_%>
    <%_ if(addMessaging) {_%>
    "@totalsoft/messaging-host": "^1.0.0",
    <%_}_%>
    "apollo-datasource": "^0.9.0",
    "apollo-datasource-rest": "^0.14.0",
    <%_ if(addTracing){ _%>
    "apollo-opentracing": "^2.1.0",
    <%_}_%>
    "apollo-server": "2.25.2",
    "apollo-server-koa": "^2.25.2",
    "async-mutex": "^0.3.1",
    "bluebird": "3.7.2",
    "bufferutil": "^4.0.3",
    "colors": "^1.4.0",
    "dataloader": "^2.0.0",
    "datasource-sql": "^1.4.1",
    "dotenv": "10.0.0",
    "get-stream": "^6.0.1",
    "graphql": "^15.5.1",
    "graphql-middleware": "^6.0.10",
    <%_ if(addSubscriptions){ _%>
    "graphql-redis-subscriptions": "^2.4.0",
    <%_}_%>
    "graphql-shield": "^7.5.0",
    <%_ if(addSubscriptions){ _%>
    "graphql-subscriptions": "^1.2.1",
    <%_}_%>
    "graphql-tools": "^7.0.5",
    "humps": "^2.0.1",
    <%_ if(addSubscriptions){ _%>
    "ioredis": "^4.27.6",
    <%_}_%>
    <%_ if(addTracing){ _%>
    "jaeger-client": "^3.18.1",
    <%_}_%>
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "^2.0.3",
    "knex": "^0.21.19",
    "knex-tiny-logger": "^2.1.0",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-jwt": "^4.0.1",
    "lodash.merge": "^4.6.2",
    "mime-types": "^2.1.31",
    "mssql": "^7.1.3",
    "node-nats-streaming": "0.3.2",
    <%_ if(addTracing){ _%>
    "opentracing": "^0.14.5",
    <%_}_%>
    "path": "^0.12.7",
    "pino": "^6.11.3",
    "ramda": "^0.27.1",
    <%_ if(addSubscriptions){ _%>
    "subscriptions-transport-ws": "^0.9.19",
    <%_}_%>
    "tedious": "^11.0.7",
    "utf-8-validate": "^5.0.5",
    "utf8-bytes": "0.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "easygraphql-tester": "^6.0.1",
    "eslint": "^7.29.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^27.0.5",
    "jest-extended": "^0.11.5",
    "jest-junit": "^12.2.0",
    "prettier": "^2.3.2",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended"
    ]
  }
}
