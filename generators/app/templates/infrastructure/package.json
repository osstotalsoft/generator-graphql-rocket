{
  "name": "<%= projectName.toLowerCase() %>",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=16.0.0",
    "<%=packageManager%>": ">= <%= packageManagerVersion %>"
  },
  "scripts": {
    "postinstall": "npx sort-package-json",
    "setcodepage": "run-script-os",
    "setcodepage:linux": "",
    "setcodepage:windows": "chcp 65001",
    "start": "npm run setcodepage && npm run lint && cross-env NODE_ENV=development nodemon src/index.js",
    "start:production": "cross-env NODE_ENV=production node src/index.js",
    "test": "jest --collectCoverage --passWithNoTests",
    "test:watchAll": "<%=packageManager%> run test -- --watchAll",
    "test:watch": "<%=packageManager%> run test -- --watch",
    "test:ci": "cross-env CI=true <%=packageManager%> test -- --reporters=default --reporters=jest-junit --coverage --coverageReporters=cobertura --coverageReporters=lcov --coverageReporters=html",
    "lint": "eslint --fix --ext .js .",
    "prettier": "prettier --write **/*.js",
    "prepack": "husky install"
  },
  "lint-staged": {
    "**/*.+(js|md|css|graphql|json)": "prettier --write"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.4.0",
    "@graphql-tools/load": "^7.7.0",
    "@graphql-tools/schema": "^8.5.0",
    "@koa/cors": "^3.3.0",
    "@koa/router": "^11.0.1",
    <%_ if(dataLayer === "prisma") {_%>
    "@prisma/client": "^4.1.1",
    "prisma": "^4.1.1",
      <%_ if(withMultiTenancy && hasSharedDb) {_%>
    "lodash.set": "^4.3.2",
      <%_} _%>
    <%_} _%>
    "@opentelemetry/exporter-prometheus": "^0.27.0",
    "@opentelemetry/sdk-metrics-base": "^0.27.0",
    "@totalsoft/key-per-file-configuration": "^1.1.0",
    "@totalsoft/graceful-shutdown": "^1.1.0",
    "@totalsoft/correlation": "^1.1.0",
    "@totalsoft/pino-apollo": "^1.1.0",
    "@totalsoft/pino-correlation": "^1.1.0",
    "@totalsoft/pino-mssqlserver": "^1.1.0",
    <%_ if(withMultiTenancy) {_%>
    "object-path": "^0.11.8",
    "koa-ignore": "^1.0.1",
      <%_ if(addQuickStart) {_%>
    "datasource-sql": "^2.0.1",
      <%_}_%>
    "@totalsoft/multitenancy-core": "^1.1.0",
    "@totalsoft/pino-multitenancy": "^1.1.0",
    <%_}_%>
    <%_ if(dataLayer === "knex") {_%>
    "dataloader": "^2.1.0",
    "knex-tiny-logger": "^2.1.0",
    "mssql": "^8.1.2",
    "knex": "^0.95.0",
      <%_ if(withMultiTenancy && hasSharedDb) {_%>
    "@totalsoft/knex-filters": "^2.4.1",
      <%_} _%>
    <%_}_%>
    <%_ if(addMessaging || (withMultiTenancy && addSubscriptions)) {_%>
    "@totalsoft/message-bus": "^2.4.2",
    "@totalsoft/messaging-host": "^2.4.2",
    <%_}_%>
    "apollo-datasource": "^3.3.2",
    "apollo-datasource-rest": "^3.6.1",
    "apollo-server": "^3.10.0",
    "apollo-server-koa": "^3.10.0",
    "async-mutex": "^0.3.2",
    "bluebird": "3.7.2",
    "colors": "^1.4.0",
    "console-stamp": "^3.0.6",
    "dotenv": "16.0.1",
    "graphql": "15.8.0",
    "graphql-middleware": "^6.1.31",
    <%_ if(addSubscriptions){ _%>
    "apollo-server-core": "^3.10.0",
    "graphql-redis-subscriptions": "^2.5.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.9.1",
    "ioredis": "^5.2.0",
    "ws": "^8.8.0",
    <%_}_%>
    <%_ if(withRights){ _%>
    "graphql-shield": "^7.5.0",
    <%_}_%>
    "graphql-upload": "^13.0.0",
    "humps": "^2.0.1",
    <%_ if(addTracing){ _%>
    "jaeger-client": "^3.19.0",
    "opentracing": "^0.14.7",
    "apollo-opentracing": "^2.1.76",
    "@totalsoft/opentracing": "^1.1.0",
    "@totalsoft/pino-opentracing": "^1.1.0",
    <%_}_%>
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "^2.1.4",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-jwt": "^4.0.3",
    "lodash.merge": "^4.6.2",
    "mime-types": "^2.1.35",
    "numeral": "^2.0.6",
    "path": "^0.12.7",
    "ramda": "^0.28.0",
    "tedious": "^14.7.0",
    "uuid": "^8.3.2",
    "pino": "^7.6.3",
    "pino-abstract-transport": "^1.0.0",
    "pino-pretty": "^8.1.0"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "easygraphql-tester": "^6.0.1",
    "eslint": "^8.19.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "jest-extended": "^3.0.0",
    "jest-junit": "^14.0.0",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "run-script-os": "^1.1.6"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended"
    ]
  },
  "nodemonConfig": {
    "ext": "js,mjs,json,graphql"
  }
}
