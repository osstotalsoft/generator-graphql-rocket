generator client {
  provider = "prisma-client-js"
  <%_ if(addTracing){ _%>
  previewFeatures = ["tracing"]
  <%_}_%>
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

// Example of generated code
model User {
  Id    Int     @id @default(autoincrement())
  FirstName  String?
  LastName  String?
}
